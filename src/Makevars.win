
pb_bundled=@pb_bundled@
CXX_STD = CXX11
PKG_CPPFLAGS =-DROUT -I$(LIB_XML2)/$(R_ARCH)/include/libxml2 -I$(LIB_PROTOBUF)/include -DLIBXML_STATIC

PKG_LIBS += $(LIB_XML2)/$(R_ARCH)/lib/libxml2.a $(LIB_PROTOBUF)/lib/$(R_ARCH)/libprotobuf.a `${R_HOME}/bin/Rscript -e "flowCore:::LdFlags()"` -lws2_32

all: 	$(SHLIB)

$(SHLIB): $(OBJECTS)


.PHONY: 	all clean

clean:
		rm -f $(OBJECTS) $(SHLIB)
